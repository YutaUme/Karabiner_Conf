{
    "title": "Personal Configulation",
    "rules": [
        {
            "description": "fn + l -> right_arrow",
            "manipulators": [
                {
                    "type": "basic",
                    "from": {
                        "key_code": "l",
                        "modifiers": {
                            "mandatory": [
                                "fn"
                            ],
                            "optional": [
                                "any"
                            ]
                        }
                    },
                    "to": [
                        {
                            "key_code": "right_arrow"
                        }
                    ]
                }
            ]
        },
        {
            "description": "fn + j -> down_arrow",
